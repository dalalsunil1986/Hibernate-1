<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>



 <class name="com.nt.entity.College" >


<!-- Singular ID field configuration -->



<id name="id" type="int" length="10">

<generator class="increment"></generator>

</id>


<property name="name" type="string" length="30" unique="true" not-null="false"/>


<property name="location" type="string" length="30" />

<property name="strength" type="int" length="10"/>

<property name="grade" type="string" length="10"/>


</class>

</hibernate-mapping>